{
	"version": 6,
	"configurePresets": [
		{
			"name": "debug",
			"displayName": "Debug",
			"generator": "Ninja",
			"binaryDir": ".build/debug",
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": "Debug",
				"CMAKE_CXX_FLAGS_INIT": "-Wall -Wextra -Wno-unused-command-line-argument -O0"
			}
		},
		{
			"name": "release",
			"displayName": "Release",
			"generator": "Ninja",
			"binaryDir": ".build/release",
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": "Release",
				"CMAKE_CXX_FLAGS_INIT": "-Werror -Wall -Wextra -Wno-unused-command-line-argument -O3"
			}
		}
	]
}
